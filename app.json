{
  "expo": {
    "name": "Safina",
    "slug": "safina",
    "owner": "triple_a",
    "projectId": "4734986f-3a0e-484b-8b34-690b1d4c7479",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "platforms": ["ios", "android", "web"],
    "splash": {
      "image": "./assets/images/appstart.png",
      "resizeMode": "cover"
    },
    "notification": {
      "icon": "./assets/images/appicon.png",
      "color": "#6A0DAD",
      "androidMode": "default",
      "androidCollapsedTitle": "Updates from Safina"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/appicon.png"
    },
    "android": {
      "package": "com.safinakids.app",
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "buildToolsVersion": "34.0.0",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "POST_NOTIFICATIONS"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appicon.png",
        "backgroundColor": "#6A0DAD"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/appicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0",
            "minSdkVersion": 23
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/appicon.png",
          "color": "#6A0DAD"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4734986f-3a0e-484b-8b34-690b1d4c7479"
      }
    }
  }
} 